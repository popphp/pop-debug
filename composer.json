{
    "name": "popphp/pop-debug",
    "description": "Pop Debug Component for Pop PHP Framework",
    "keywords": [
        "debugger",
        "php debugger",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "popphp/pop-db" : "^4.1.3",
        "popphp/pop-log" : "^3.0.2",
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Debug\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
