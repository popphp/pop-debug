{
    "name": "popphp/pop-debug",
    "description": "Pop Debug Component for Pop PHP Framework",
    "keywords": [
        "debugger",
        "php debugger",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-debug",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.3.0",
        "popphp/pop-csv": "^4.2.1",
        "popphp/pop-db": "^6.7.0",
        "popphp/pop-http": "^5.3.8",
        "popphp/pop-log": "^4.0.4",
        "popphp/pop-session": "^4.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Debug\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
