{
    "name": "popphp/pop-debug",
    "description": "Pop Debug Component for Pop PHP Framework",
    "keywords": [
        "debugger",
        "php debugger",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "http://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "popphp/pop-db": "^5.3.8",
        "popphp/pop-http": "^4.2.0",
        "popphp/pop-session": "^3.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Debug\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
